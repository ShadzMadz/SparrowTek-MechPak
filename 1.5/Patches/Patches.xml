<Patch>

<!-- Heavy Mech Gestation Recipe Patches -->

<Operation Class="PatchOperationAdd">
    <xpath>/Defs</xpath>
  <value>
  <RecipeDef ParentName="HeavyMechanoidRecipe">
    <defName>Mech_TekCentipedeBlaster</defName>
    <label>gestate Tek Centipede Blaster</label>
    <description>Gestate a Sparrow Tek Centipede Blaster mechanoid.</description>
    <gestationCycles>6</gestationCycles>
    <researchPrerequisite>UltraMechtech</researchPrerequisite>
    <ingredients Inherit="False">
      <li>
        <filter>
          <thingDefs>
            <li>Steel</li>
          </thingDefs>
        </filter>
        <count>400</count>
      </li>
      <li>
        <filter>
          <thingDefs>
            <li>Plasteel</li>
          </thingDefs>
        </filter>
        <count>400</count>
      </li>
      <li>
        <filter>
          <thingDefs>
            <li>Uranium</li>
          </thingDefs>
        </filter>
        <count>175</count>
      </li>
      <li>
        <filter>
          <thingDefs>
            <li>ComponentSpacer</li>
          </thingDefs>
        </filter>
        <count>5</count>
      </li>
      <li>
        <filter>
          <thingDefs>
            <li>SubcoreHigh</li>
          </thingDefs>
        </filter>
        <count>1</count>
      </li>
    </ingredients>
    <products>
      <Mech_TekCentipedeBlaster>1</Mech_TekCentipedeBlaster>
    </products>
    <descriptionHyperlinks>
      <ThingDef>Mech_TekCentipedeBlaster</ThingDef>
    </descriptionHyperlinks>
  </RecipeDef>
	  </value>
</Operation>

<Operation Class="PatchOperationAdd">
  <xpath>Defs/ThingDef[defName="LargeMechGestator"]/recipes</xpath>
  <order>Prepend</order>
  <value>
      <li>Mech_TekCentipedeBlaster</li>
  </value>
</Operation>

<!-- Heavy Mech Ressurection Recipe Patches -->

	<Operation Class="PatchOperationInsert">
