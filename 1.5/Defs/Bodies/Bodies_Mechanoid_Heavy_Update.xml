  <!-- Defender -->
  <!-- Tek Defender â€” Cleaned & Optimized BodyDef -->
<BodyDef>
  <defName>Mech_TekDefender</defName>
  <label>Tek Defender</label>
  <corePart>
    <def>MechanicalThorax</def>
    <coverage>1.0</coverage>
    <height>Middle</height>
    <depth>Outside</depth>
    <groups>
      <li>CoveredByShield</li>
    </groups>
    <parts>

      <!-- 1) Front Shield Plate -->
      <li>
        <def>FrontShield</def>
        <coverage>0.85</coverage>
        <height>Middle</height>
        <depth>Outside</depth>
        <groups>
          <li>CoveredByShield</li>
        </groups>
        <parts>
          <li>
            <def>ShieldWindow</def>
            <coverage>0.75</coverage>
            <height>Middle</height>
            <depth>Outside</depth>
            <groups>
              <li>gunBarrel</li>
            </groups>
          </li>
          <li>
            <def>gunBarrel</def>
            <customLabel>Left Gun Barrel</customLabel>
            <coverage>0.05</coverage>
            <height>Middle</height>
            <depth>Outside</depth>
            <groups>
              <li>gunBarrel</li>
            </groups>
          </li>
          <li>
            <def>gunBarrel</def>
            <customLabel>Right Gun Barrel</customLabel>
            <coverage>0.05</coverage>
            <height>Middle</height>
            <depth>Outside</depth>
            <groups>
              <li>gunBarrel</li>
            </groups>
          </li>
        </parts>
      </li>

      <!-- 2) Neck & Head (behind shield) -->
      <li>
        <def>MechanicalNeck</def>
        <coverage>0.05</coverage>
        <height>Top</height>
        <depth>Outside</depth>
        <groups>
          <li>CoveredByShield</li>
        </groups>
        <parts>
          <li>
            <def>MechanicalHead</def>
            <coverage>0.25</coverage>
            <height>Top</height>
            <depth>Outside</depth>
            <parts>
              <li>
                <def>ArtificialBrain</def>
                <coverage>0.05</coverage>
                <depth>Inside</depth>
              </li>
              <li>
                <def>Reactor</def>
                <customLabel>Reactor 1</customLabel>
                <coverage>0.05</coverage>
                <depth>Inside</depth>
              </li>
              <li>
                <def>FluidReprocessor</def>
                <coverage>0.05</coverage>
                <depth>Inside</depth>
              </li>
              <li>
                <def>SightSensor</def>
                <customLabel>Left Sight Sensor</customLabel>
                <coverage>0.03</coverage>
                <depth>Outside</depth>
              </li>
              <li>
                <def>SightSensor</def>
                <customLabel>Right Sight Sensor</customLabel>
                <coverage>0.03</coverage>
                <depth>Outside</depth>
              </li>
              <li>
                <def>HearingSensor</def>
                <customLabel>Left Hearing Sensor</customLabel>
                <coverage>0.02</coverage>
                <depth>Outside</depth>
              </li>
              <li>
                <def>HearingSensor</def>
                <customLabel>Right Hearing Sensor</customLabel>
                <coverage>0.02</coverage>
                <depth>Outside</depth>
              </li>
              <li>
                <def>SmellSensor</def>
                <coverage>0.02</coverage>
                <depth>Outside</depth>
              </li>
            </parts>
          </li>
        </parts>
      </li>

      <!-- 3) Left Arm -->
      <li>
        <def>MechanicalShoulder</def>
        <coverage>0.05</coverage>
        <customLabel>Left Shoulder</customLabel>
        <height>Middle</height>
        <depth>Outside</depth>
        <groups>
          <li>CoveredByShield</li>
        </groups>
        <parts>
          <li>
            <def>MechanicalArm</def>
            <coverage>0.60</coverage>
            <customLabel>Left Arm</customLabel>
            <parts>
              <li>
                <def>MechanicalHand</def>
                <coverage>0.10</coverage>
                <customLabel>Left Hand</customLabel>
                <height>Bottom</height>
                <parts>
                  <li>
                    <def>MechanicalFinger</def>
                    <coverage>0.025</coverage>
                    <customLabel>Left Pinky</customLabel>
                    <groups><li>LeftHand</li></groups>
                  </li>
                  <li>
                    <def>MechanicalFinger</def>
                    <coverage>0.025</coverage>
                    <customLabel>Left Middle Finger</customLabel>
                    <groups><li>LeftHand</li></groups>
                  </li>
                  <li>
                    <def>MechanicalFinger</def>
                    <coverage>0.025</coverage>
                    <customLabel>Left Index Finger</customLabel>
                    <groups><li>LeftHand</li></groups>
                  </li>
                  <li>
                    <def>MechanicalFinger</def>
                    <coverage>0.025</coverage>
                    <customLabel>Left Thumb</customLabel>
                    <groups><li>LeftHand</li></groups>
                  </li>
                </parts>
              </li>
            </parts>
          </li>
        </parts>
      </li>

      <!-- 4) Right Arm -->
      <li>
        <def>MechanicalShoulder</def>
        <coverage>0.05</coverage>
        <customLabel>Right Shoulder</customLabel>
        <height>Middle</height>
        <depth>Outside</depth>
        <groups>
          <li>CoveredByShield</li>
        </groups>
        <parts>
          <li>
            <def>MechanicalArm</def>
            <coverage>0.60</coverage>
            <customLabel>Right Arm</customLabel>
            <parts>
              <li>
                <def>MechanicalHand</def>
                <coverage>0.10</coverage>
                <customLabel>Right Hand</customLabel>
                <height>Bottom</height>
                <parts>
                  <li>
                    <def>MechanicalFinger</def>
                    <coverage>0.025</coverage>
                    <customLabel>Right Pinky</customLabel>
                    <groups><li>RightHand</li></groups>
                  </li>
                  <li>
                    <def>MechanicalFinger</def>
                    <coverage>0.025</coverage>
                    <customLabel>Right Middle Finger</customLabel>
                    <groups><li>RightHand</li></groups>
                  </li>
                  <li>
                    <def>MechanicalFinger</def>
                    <coverage>0.025</coverage>
                    <customLabel>Right Index Finger</customLabel>
                    <groups><li>RightHand</li></groups>
                  </li>
                  <li>
                    <def>MechanicalFinger</def>
                    <coverage>0.025</coverage>
                    <customLabel>Right Thumb</customLabel>
                    <groups><li>RightHand</li></groups>
                  </li>
                </parts>
              </li>
            </parts>
          </li>
        </parts>
      </li>

      <!-- 5) Left Leg -->
      <li>
        <def>MechanicalLeg</def>
        <coverage>0.05</coverage>
        <height>Bottom</height>
        <customLabel>Left Leg</customLabel>
        <groups>
          <li>CoveredByShield</li>
        </groups>
        <parts>
          <li>
            <def>MechanicalFoot</def>
            <coverage>0.05</coverage>
            <customLabel>Left Foot</customLabel>
          </li>
        </parts>
      </li>

      <!-- 6) Right Leg -->
      <li>
        <def>MechanicalLeg</def>
        <coverage>0.05</coverage>
        <height>Bottom</height>
        <customLabel>Right Leg</customLabel>
        <groups>
          <li>CoveredByShield</li>
        </groups>
        <parts>
          <li>
            <def>MechanicalFoot</def>
            <coverage>0.05</coverage>
            <customLabel>Right Foot</customLabel>
          </li>
        </parts>
      </li>

    </parts>
  </corePart>
</BodyDef>


</Defs>
